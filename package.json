{
  "name": "react-rollup-npm-boilerplate",
  "version": "1.0.6",
  "description": "An example of creating a react npm package",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "commit": "git add . && cz && standard-version",
    "build-storybook": "storybook build",
    "build-lib": "rollup -c --bundleConfigAsCjs",
    "test": "jest -c jest/jest.config.js --verbose",
    "test:watch": "pnpm run test --watch",
    "test:clear": "pnpm run test --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DonAdam2/react-rollup-npm-boilerplate.git"
